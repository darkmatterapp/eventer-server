<%= render "/shared/notice" %>

<div class="page-header">
  <h1>Events</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>&nbsp;</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.name, event %></td>
        <td><%= link_to "Edit", edit_event_path(event), class: "btn btn-primary btn-xs" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
