<p id="notice"><%= notice %></p>

<h1>Listing Media</h1>

<table>
  <thead>
    <tr>
      <th>Image url</th>
      <th>Instagram url</th>
      <th>Flick url</th>
      <th>Facebook url</th>
      <th>Audio url</th>
      <th>Soundcloud url</th>
      <th>Video url</th>
      <th>Youtube url</th>
      <th>Vimeo url</th>
      <th>Title</th>
      <th>Description</th>
      <th>Creator name</th>
      <th>Creator url</th>
      <th>Published at</th>
      <th>Private</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @media.each do |medium| %>
      <tr>
        <td><%= medium.image_url %></td>
        <td><%= medium.instagram_url %></td>
        <td><%= medium.flick_url %></td>
        <td><%= medium.facebook_url %></td>
        <td><%= medium.audio_url %></td>
        <td><%= medium.soundcloud_url %></td>
        <td><%= medium.video_url %></td>
        <td><%= medium.youtube_url %></td>
        <td><%= medium.vimeo_url %></td>
        <td><%= medium.title %></td>
        <td><%= medium.description %></td>
        <td><%= medium.creator_name %></td>
        <td><%= medium.creator_url %></td>
        <td><%= medium.published_at %></td>
        <td><%= medium.private %></td>
        <td><%= medium.user_id %></td>
        <td><%= link_to 'Show', medium %></td>
        <td><%= link_to 'Edit', edit_medium_path(medium) %></td>
        <td><%= link_to 'Destroy', medium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medium', new_medium_path %>
