<%= form_for [:admin, @event, @venue, @location] do |f| %>
  <%= render "shared/form_errors", resource: @location %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, autofocus: true, class: "form-control input-lg" %>
  </div>

  <div class="form-group">
    <%= f.label :venue_id %>
    <%= f.select :venue_id, venue_options_for_select(@event), {}, class: "form-control" %>
  </div>

  <%= render "shared/form_actions", f: f %>
<% end %>
