<dl>
  <% event.sessions.each do |session| %>
    <dt><%= link_to session.name, [event, session] %></dt>

    <% if session.participants.present? %>
      <% session.participants.each do |participant| %>
        <dd><%= link_to participant.name, [event, participant] %></dd>
      <% end %>
    <% end %>

  <% end %>
</dl>
